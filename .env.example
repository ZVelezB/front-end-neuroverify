# Auth service (likely from auth_google_c)
VITE_AUTH_API_BASE="https://auth.example.com"
# EEG data service (likely from server_eeg)
VITE_DATA_API_BASE="https://eeg.example.com"

# If backend exposes a direct Google start URL, use it:
# e.g., https://auth.example.com/auth/google?redirect_uri=https://app.example.com/auth/callback
VITE_AUTH_GOOGLE_URL="https://auth.example.com/auth/google"

# Front-end callback path (must be allowed server-side)
VITE_AUTH_CALLBACK_PATH="/auth/callback"

# Choose how token is transported: "cookie" | "bearer"
VITE_AUTH_TOKEN_MODE="bearer"
VITE_FAKE_AUTH=true